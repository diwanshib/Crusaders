import pandas as pd

# We'll also import seaborn, a Python graphing library
import warnings # current version of seaborn generates a bunch of warnings that we'll ignore
warnings.filterwarnings("ignore")
import seaborn as sns
import matplotlib.pyplot as plt
sns.set(style="dark", color_codes=True)

# Next, we'll load the dataset, which is in the "../input/" directory
df = pd.read_csv("sales_data_sample.csv") # the dataset is now a Pandas DataFrame

# Let's see what's in the data - Jupyter notebooks print the result of the last thing you do
df.head()
df_pm=df[df['PRODUCTLINE']=='Motorcycles']
sns.FacetGrid(df_pm, hue="COUNTRY", height=8) \
   .map(sns.kdeplot, "QUANTITYORDERED").add_legend()
df_Ire = df[df['COUNTRY'] == 'Ireland']
plt.plot(df_Ire['ORDERNUMBER'], df_Ire['DEALSIZE'],color='g')

